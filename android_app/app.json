{
  "expo": {
    "name": "Dripdirective",
    "slug": "dripdirective-android",
    "version": "1.0.0",
    "scheme": "dripdirective",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F1A"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dripdirective.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0F0F1A"
      },
      "package": "com.dripdirective.app",
      "versionCode": 1
    },
    "extra": {
      "eas": {
        "projectId": "71556aec-524b-4d47-a220-b228a8b33606"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
